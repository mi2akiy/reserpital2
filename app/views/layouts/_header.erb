<header class="sticky-top bg-info">
  <nav class="navbar navbar-expand-md navbar-dark bg-info text-white">
    <a class="logo">
      <%= link_to 'reserpital', root_path, data: {"turbolinks" => false}, :style=>"color:white;" %>
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavDropdown">
      <!--<div class="search">-->
      <!--  <%#= render 'public/hospitals/search', q: @q, url: public_hospitals_path %>-->
      <!--</div>-->
      <ul class="navbar-nav navbar-dark">
        <% if admin_signed_in? %>
            <li class="nav-list">
              <%= link_to 'ユーザー一覧', admin_end_users_path %>
            </li>
            <li class="nav-list">
              <%= link_to '病院一覧', admin_hospitals_path %>
            </li>
            <li class="nav-list">
              <%= link_to '診療科一覧', admin_clinical_departments_path %>
            </li>
            <li class="nav-list">
              <%= link_to '病院を追加', new_admin_hospital_path %>
            </li>
            <li class="nav-list">
              <%= link_to 'ログアウト', destroy_admin_session_path, method: :delete %>
            </li>
          <% elsif owner_signed_in? %>
            <li class="nav-list">
              <%= link_to '病院詳細', owner_hospital_path(current_owner.hospital.id) %>
            </li>
            <li class="nav-list">
              <%= link_to 'オーナー一覧', owner_owners_path %>
            </li>
            <li class="nav-list">
              <%= link_to '診察予約一覧', owner_reservations_path %>
            </li>
            <li class="nav-list">
              <%= link_to 'ログアウト', destroy_owner_session_path, method: :delete %>
            </li>
          <% elsif end_user_signed_in? %>
            <li class="nav-list">
              <%= link_to '病院を探す', public_hospitals_path %>
            </li>
            <li class="nav-list">
              <%= link_to 'マイページ', public_end_users_path %>
            </li>
            <li class="nav-list">
              <%= link_to 'ログアウト', destroy_end_user_session_path, method: :delete %>
            </li>
          <% else %>
            <li class="nav-list">
              <%= link_to '病院を探す', public_hospitals_path %>
            </li>
            <li class="nav-list">
              <%= link_to '新規会員登録', new_end_user_registration_path %>
            </li>
            <li class="nav-list">
              <%= link_to 'ログイン', new_end_user_session_path %>
            </li>
          <% end %>
        </ul>
    </div>
  </nav>
</header>

